<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ParkEase.Page.TabbedPageFirst">

    
    
    <!-- Tab 1 -->
    <ContentPage Title="Tab 1">
        <StackLayout Padding="10" BackgroundColor="#EFEFEF">
            <Label Text="Content for Tab 1" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage>

    <!-- Tab 2 -->
    <ContentPage Title="Tab 2">
        <StackLayout Padding="10" BackgroundColor="#EFEFEF">
            <Label Text="Content for Tab 2" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage>
</TabbedPage>
